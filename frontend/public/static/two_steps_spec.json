{
    "pipelineSpec": {
      "deploymentSpec": {
        "executors": {
          "exec-consumer": {
            "container": {
              "image": "google/cloud-sdk:latest",
              "command": [
                "sh",
                "-c",
                "set -e -x\necho \"Read from an input parameter: \" && echo \"$0\"\necho \"Read from an input artifact: \" && gsutil cat \"$1\"\n",
                "{{$.inputs.parameters['input_value']}}",
                "{{$.inputs.artifacts['input_artifact'].uri}}"
              ]
            }
          },
          "exec-producer": {
            "container": {
              "image": "google/cloud-sdk:latest",
              "command": [
                "sh",
                "-c",
                "set -e -x\necho \"$0, this is an output parameter\" | gsutil cp - \"$1\"\necho \"$0, this is an output artifact\" | gsutil cp - \"$2\"\n",
                "{{$.inputs.parameters['input_text']}}",
                "{{$.outputs.parameters['output_value'].output_file}}",
                "{{$.outputs.artifacts['output_artifact'].uri}}"
              ]
            }
          }
        }
      },
      "pipelineInfo": {
        "name": "simple-two-step-pipeline-jamxl-1613756626"
      },
      "sdkVersion": "kfp-1.5.0.dev0",
      "components": {
