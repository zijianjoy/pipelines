{
    "pipelineSpec": {
      "schemaVersion": "2.0.0",
      "deploymentSpec": {
        "executors": {
          "exec-hello-world": {
            "container": {
              "image": "python:3.7",
              "args": [
                "--text",
                "{{$.inputs.parameters['text']}}"
              ],
              "command": [
                "sh",
                "-ec",
                "program_path=$(mktemp)\nprintf \"%s\" \"$0\" > \"$program_path\"\npython3 -u \"$program_path\" \"$@\"\n",
                "def hello_world(text):\n    print(text)\n    return text\n\nimport argparse\n_parser = argparse.ArgumentParser(prog='Hello world', description='')\n_parser.add_argument(\"--text\", dest=\"text\", type=str, required=True, default=argparse.SUPPRESS)\n_parsed_args = vars(_parser.parse_args())\n\n_outputs = hello_world(**_parsed_args)\n"
              ]
            }
          }
        }
      },
      "pipelineInfo": {
        "name": "hello-world"
      },
      "root": {
        "dag": {
          "tasks": {
            "task-hello-world": {
              "componentRef": {
                "name": "comp-hello-world"
              },
              "inputs": {
                "parameters": {
                  "text": {
                    "componentInputParameter": "input_value"
                  }
                }
              },
              "taskInfo": {
                "name": "task-hello-world"
              }
            }
          }
        },
        "inputDefinitions": {
          "parameters": {
            "input_value": {
              "type": "STRING"
            }
          }
        }
      },
      "sdkVersion": "kfp-1.5.0.dev0",
      "components": {
        "comp-hello-world": {
          "inputDefinitions": {
            "parameters": {
              "text": {
                "type": "STRING"
              }
            }
          },
          "executorLabel": "exec-hello-world"
        }
      }
    },
    "runtimeConfig": {
      "parameters": {
        "input_value": {
          "stringValue": "hello kfp"
        }
      },
      "gcsOutputDirectory": "gs://bucket/blob"
    }
  }